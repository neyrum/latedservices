{"ast":null,"code":"import axios from '@/plugins/axios';\nimport Navbar from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\nexport default {\n  name: 'Dashboard',\n  components: {\n    Navbar,\n    Sidebar,\n    Footer,\n    Notification\n  },\n  data() {\n    return {\n      // Datos para Admin Dashboard\n      summary: {\n        totalRequests: 0,\n        activeServices: 0,\n        pendingRequests: 0\n      },\n      errorMessage: '',\n      // Datos para User Dashboard (estos son ejemplos; puedes reemplazarlos con datos reales)\n      userServicesCount: 5,\n      userPendingCount: 3\n    };\n  },\n  computed: {\n    // Se asume que la información del usuario está en el store.\n    userData() {\n      return this.$store.getters[\"auth/userData\"] || {};\n    },\n    userRole() {\n      return this.userData.role || 'user'; // Rol predeterminado: 'user'\n    },\n    isAdmin() {\n      return this.userRole === \"admin\";\n    },\n    isUser() {\n      return this.userRole === \"user\";\n    },\n    // Preparar un array de cards para el admin dashboard\n    adminCards() {\n      return [{\n        title: 'Total Requests',\n        value: this.summary.totalRequests || 0\n      }, {\n        title: 'Active Services',\n        value: this.summary.activeServices || 0\n      }, {\n        title: 'Pending Requests',\n        value: this.summary.pendingRequests || 0\n      }];\n    }\n  },\n  async created() {\n    // Solo para administradores queremos cargar el resumen con datos desde la API.\n    if (this.isAdmin) {\n      await this.fetchSummary();\n    }\n    // Para usuarios regulares se pueden agregar otras peticiones o lógica adicional.\n  },\n  methods: {\n    async fetchSummary() {\n      try {\n        const response = await axios.get('/admin/summary');\n        this.summary = response.data;\n      } catch (err) {\n        this.errorMessage = err.response?.data?.message || 'Failed to load dashboard data.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Navbar","Footer","name","components","Sidebar","Notification","data","summary","totalRequests","activeServices","pendingRequests","errorMessage","userServicesCount","userPendingCount","computed","userData","$store","getters","userRole","role","isAdmin","isUser","adminCards","title","value","created","fetchSummary","methods","response","get","err","message"],"sources":["/Users/macbookpro/project/LatedServices/frontend/src/views/admin/Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <Navbar />\r\n    <Sidebar />\r\n    <div class=\"content-wrapper\">\r\n      <div class=\"container-fluid mt-4\">\r\n      \r\n        <!-- Dashboard para Administradores -->\r\n        <template v-if=\"isAdmin\">\r\n          <h2>Admin Dashboard</h2>\r\n          <Notification v-if=\"errorMessage\" type=\"danger\" :message=\"errorMessage\" />\r\n          <div class=\"row\">\r\n            <div class=\"col-md-4\" v-for=\"card in adminCards\" :key=\"card.title\">\r\n              <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                  <h5 class=\"card-title\">{{ card.title }}</h5>\r\n                  <p class=\"card-text\">{{ card.value }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </template>\r\n        \r\n        <!-- Dashboard para usuarios regulares -->\r\n        <template v-else-if=\"isUser\">\r\n          <h2>User Dashboard</h2>\r\n          <p>Bienvenido, {{ userData.name }}. Aquí tienes tu información personalizada.</p>\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6\">\r\n              <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                  <h5 class=\"card-title\">Tus servicios activos</h5>\r\n                  <p class=\"card-text\">{{ userServicesCount || 0 }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-6\">\r\n              <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                  <h5 class=\"card-title\">Solicitudes pendientes</h5>\r\n                  <p class=\"card-text\">{{ userPendingCount || 0 }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </template>\r\n        \r\n        <!-- Dashboard genérico para otros roles -->\r\n        <template v-else>\r\n          <h2>Dashboard</h2>\r\n          <p>Tu rol no tiene contenido específico.</p>\r\n        </template>\r\n        \r\n      </div>\r\n    </div>\r\n    <Footer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/plugins/axios';\r\nimport Navbar from '@/components/Navbar.vue';\r\nimport Footer from '@/components/Footer.vue';\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  components: { Navbar, Sidebar, Footer, Notification },\r\n  data() {\r\n    return {\r\n      // Datos para Admin Dashboard\r\n      summary: {\r\n        totalRequests: 0,\r\n        activeServices: 0,\r\n        pendingRequests: 0,\r\n      },\r\n      errorMessage: '',\r\n      // Datos para User Dashboard (estos son ejemplos; puedes reemplazarlos con datos reales)\r\n      userServicesCount: 5,\r\n      userPendingCount: 3,\r\n    };\r\n  },\r\n  computed: {\r\n    // Se asume que la información del usuario está en el store.\r\n    userData() {\r\n      return this.$store.getters[\"auth/userData\"] || {};\r\n    },\r\n    userRole() {\r\n      return this.userData.role || 'user'; // Rol predeterminado: 'user'\r\n    },\r\n    isAdmin() {\r\n      return this.userRole === \"admin\";\r\n    },\r\n    isUser() {\r\n      return this.userRole === \"user\";\r\n    },\r\n    // Preparar un array de cards para el admin dashboard\r\n    adminCards() {\r\n      return [\r\n        { title: 'Total Requests', value: this.summary.totalRequests || 0 },\r\n        { title: 'Active Services', value: this.summary.activeServices || 0 },\r\n        { title: 'Pending Requests', value: this.summary.pendingRequests || 0 },\r\n      ];\r\n    },\r\n  },\r\n  async created() {\r\n    // Solo para administradores queremos cargar el resumen con datos desde la API.\r\n    if (this.isAdmin) {\r\n      await this.fetchSummary();\r\n    }\r\n    // Para usuarios regulares se pueden agregar otras peticiones o lógica adicional.\r\n  },\r\n  methods: {\r\n    async fetchSummary() {\r\n      try {\r\n        const response = await axios.get('/admin/summary');\r\n        this.summary = response.data;\r\n      } catch (err) {\r\n        this.errorMessage = err.response?.data?.message || 'Failed to load dashboard data.';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh;\r\n}\r\n.content-wrapper {\r\n  flex: 1;\r\n  padding: 20px;\r\n}\r\n.card {\r\n  border-radius: 0.25rem;\r\n}\r\n</style>\r\n"],"mappings":"AA4DA,OAAOA,KAAI,MAAO,iBAAiB;AACnC,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEH,MAAM;IAAEI,OAAO;IAAEH,MAAM;IAAEI;EAAa,CAAC;EACrDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,OAAO,EAAE;QACPC,aAAa,EAAE,CAAC;QAChBC,cAAc,EAAE,CAAC;QACjBC,eAAe,EAAE;MACnB,CAAC;MACDC,YAAY,EAAE,EAAE;MAChB;MACAC,iBAAiB,EAAE,CAAC;MACpBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAAC,eAAe,KAAK,CAAC,CAAC;IACnD,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACH,QAAQ,CAACI,IAAG,IAAK,MAAM,EAAE;IACvC,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,OAAO,IAAI,CAACF,QAAO,KAAM,OAAO;IAClC,CAAC;IACDG,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACH,QAAO,KAAM,MAAM;IACjC,CAAC;IACD;IACAI,UAAUA,CAAA,EAAG;MACX,OAAO,CACL;QAAEC,KAAK,EAAE,gBAAgB;QAAEC,KAAK,EAAE,IAAI,CAACjB,OAAO,CAACC,aAAY,IAAK;MAAE,CAAC,EACnE;QAAEe,KAAK,EAAE,iBAAiB;QAAEC,KAAK,EAAE,IAAI,CAACjB,OAAO,CAACE,cAAa,IAAK;MAAE,CAAC,EACrE;QAAEc,KAAK,EAAE,kBAAkB;QAAEC,KAAK,EAAE,IAAI,CAACjB,OAAO,CAAC