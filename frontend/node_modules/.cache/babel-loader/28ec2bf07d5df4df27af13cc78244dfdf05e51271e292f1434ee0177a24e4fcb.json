agePath;\n      } catch (error) {\n        console.error(\"No se pudo cargar la imagen:\", imagePath, error);\n        // Retorna una imagen por defecto en caso de error.\n        return require(\"@/assets/img/default-profile.jpg\");\n      }\n    };\n\n    onMounted(() => {\n      // Aquí podrías agregar lógica adicional, por ejemplo, obtener actualizaciones de la API.\n    });\n\n    return { teamList, resolveImage };\n  },\n};\n</script>\n"],"mappings":"AAwCA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,WAAU,QAAS,iBAAiB;AAE7C,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAKA,CAAA,EAAG;IACN;IACA;IACA;IACA,MAAMC,QAAO,GAAIL,GAAG,CAACE,WAAW,CAAC;;IAEjC;;;;IAIA,MAAMI,YAAW,GAAKC,SAAS,IAAK;MAClC,IAAI;QACF;QACA,IAAIA,SAAS,CAACC,UAAU,CAAC,UAAU,CAAC,EAAE;UACpC,OAAOC,OAAO,CAAC,GAAGF,SAAS,EAAE,CAAC;QAChC;QACA;QACA,OAAOA,SAAS;MAClB,EAAE,OAAOG,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEH,SAAS,EAAEG,KAAK,CAAC;QAC/D;QACA,OAAOD,OAAO,CAAC,kCAAkC,CAAC;MACpD;IACF,CAAC;IAEDR,SAAS,CAAC,MAAM;MACd;IAAA,CACD,CAAC;IAEF,OAAO;MAAEI,QAAQ;MAAEC;IAAa,CAAC;EACnC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}