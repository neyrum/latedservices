{"ast":null,"code":"export default {\n  name: 'UserMenu',\n  props: {\n    userName: {\n      type: String,\n      required: true\n    },\n    userProfilePicture: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      dropdownOpen: false\n    };\n  },\n  mounted() {\n    document.addEventListener('click', this.handleClickOutside);\n  },\n  beforeDestroy() {\n    document.removeEventListener('click', this.handleClickOutside);\n  },\n  methods: {\n    toggleDropdown(event) {\n      this.dropdownOpen = !this.dropdownOpen;\n      // Dispara evento para cerrar las notificaciones.\n      window.dispatchEvent(new CustomEvent('closeNotifications'));\n      console.log('Toggle dropdown:', this.dropdownOpen);\n    },\n    handleClickOutside(event) {\n      if (!this.$el.contains(event.target)) {\n        this.dropdownOpen = false;\n      }\n    },\n    logout() {\n      this.$emit('logout');\n    },\n    handleImageError(event) {\n      event.target.src = '/path/to/default-image.jpg';\n    }\n  }\n};","map":{"version":3,"names":["name","props","userName","type","String","required","userProfilePicture","data","dropdownOpen","mounted","document","addEventListener","handleClickOutside","beforeDestroy","removeEventListener","methods","toggleDropdown","event","window","dispatchEvent","CustomEvent","console","log","$el","contains","target","logout","$emit","handleImageError","src"],"sources":["/Users/macbookpro/project/LatedServices/frontend/src/components/UserMenu.vue"],"sourcesContent":["<template>\n  <div class=\"user-menu d-flex align-items-center\">\n    <span class=\"text-light me-2\">Bienvenido, {{ userName }}</span>\n    <div class=\"dropdown\">\n      <a\n        class=\"nav-link dropdown-toggle d-flex align-items-center\"\n        href=\"#\"\n        id=\"userDropdown\"\n        role=\"button\"\n        @click.prevent.stop=\"toggleDropdown\"\n        :aria-expanded=\"dropdownOpen\"\n      >\n        <img\n          :src=\"userProfilePic