false;\n      }\n    },\n    logout() {\n      this.$emit('logout');\n    },\n    handleImageError(event) {\n      event.target.src = '/path/to/default-image.jpg';\n    }\n  }\n};\n</script>\n  \n<style scoped>\n.user-menu {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n}\n.dropdown {\n  position: relative;\n}\n.nav-link.dropdown-toggle {\n  cursor: pointer;\n}\n.rounded-circle {\n  object-fit: cover;\n}\n/* Resto de estilos para el UserMenu */\n.dropdown-menu {\n  background-color: #ffffff;\n  border: 1px solid #cccccc;\n  border-radius: 6px;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);\n  padding: 5px 0;\n  z-index: 2000;\n  min-width: 150px;\n  position: absolute;\n  top: 100%;\n  right: 0;\n  margin-top: 8px;\n  display: none;\n}\n.dropdown-menu.show {\n  display: block;\n}\n.dropdown-item {\n  padding: 8px 12px;\n  color: #333333;\n  transition: background-color 0.3s ease;\n  text-decoration: none;\n  display: block;\n}\n.dropdown-item:hover,\n.dropdown-item:focus {\n  background-color: #f0f0f0;\n  color: #202020;\n}\n.dropdown-item.active {\n  background-color: #345996;\n  color: #ffffff;\n}\n.dropdown-item button {\n  background: none;\n  border: none;\n  padding: 0;\n  color: inherit;\n  width: 100%;\n  text-align: left;\n}\n</style>\n"],"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,kBAAkB,EAAE;MAClBH,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,QAAQ,CAACC,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACC,kBAAkB,CAAC;EAC7D,CAAC;EACDC,aAAaA,CAAA,EAAG;IACdH,QAAQ,CAACI,mBAAmB,CAAC,OAAO,EAAE,IAAI,CAACF,kBAAkB,CAAC;EAChE,CAAC;EACDG,OAAO,EAAE;IACPC,cAAcA,CAACC,KAAK,EAAE;MACpB,IAAI,CAACT,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;MACtC;MACAU,MAAM,CAACC,aAAa,CAAC,IAAIC,WAAW,CAAC,oBAAoB,CAAC,CAAC;MAC3DC,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACd,YAAY,CAAC;IACpD,CAAC;IACDI,kBAAkBA,CAACK,KAAK,EAAE;MACxB,I