{"ast":null,"code":"import { useStore } from \"vuex\";\nimport { useRouter } from \"vue-router\";\nimport { computed } from \"vue\";\nimport UserMenu from \"./UserMenu.vue\";\nimport Notifications from \"./Notifications.vue\";\nexport default {\n  components: {\n    UserMenu,\n    Notifications\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const isAuthenticated = computed(() => store.getters[\"auth/isAuthenticated\"]);\n    const userName = computed(() => store.getters[\"auth/userData\"]?.name || \"Usuario\");\n    const userProfilePicture = computed(() => store.getters[\"auth/userData\"]?.profilePicture || \"default-profile.jpg\");\n    const isServiciosDisponiblesPage = computed(() => router.currentRoute.value.path === \"/client/services\");\n    const isDashboardPage = computed(() => router.currentRoute.value.path === \"/dashboard\");\n    const isHomePage = computed(() => router.currentRoute.value.path === \"/\");\n    const logout = () => {\n      store.dispatch(\"auth/logout\");\n      router.push(\"/login\");\n    };\n    return {\n      isAuthenticated,\n      userName,\n      userProfilePicture,\n      logout,\n      isServiciosDisponiblesPage,\n      isHomePage,\n      isDashboardPage\n    };\n  }\n};","map":{"version":3,"names":["useStore","useRouter","computed","UserMenu","Notifications","components","setup","store","router","isAuthenticated","getters","userName","name","userProfilePicture","profilePicture","isServiciosDisponiblesPage","currentRoute","value","path","isDashboardPage","isHomePage","logout","dispatch","push"],"sources":["/Users/macbookpro/project/LatedServices/frontend/src/components/Navbar.vue"],"sourcesContent":["<template>\n  <nav class=\"navbar navbar-expand-lg navbar-dark bg-primary fixed-top\">\n    <div class=\"container\">\n      <router-link class=\"navbar-brand fw-bold text-uppercase\" to=\"/\">Servicios de Lated</router-link>\n\n      <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\">\n        <span class=\"navbar-toggler-icon\"></span>\n      </button>\n\n      <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\n        <ul class=\"navbar-nav ms-auto\">\n          <template v-if=\"!isServiciosDisponiblesPage && !isDashboardPage\">\n            <li class=\"nav-item\"><a class=\"nav-link\" href=\"#services\">Servicios</a></li>\n            <li class=\"nav-item\"><a class=\"nav-link\" href=\"#portfolio\">Portafolio</a></li>\n            <li class=\"nav-item\"><a class=\"nav-link\" href=\"#about\">Sobre Nosotros</a></li>\n            <li class=\"nav-item\"><a class=\"nav-link\" href=\"#team\">Equipo</a></li>\n            <li class=\"nav-item\"><a class=\"nav-link\" href=\"#contact\">Contacto</a></li>\n          </template>\n\n          <li class=\"nav-item\" v-if=\"!isHomePage\">\n            <router-link class=\"nav-link\" to=\"/\">\n              <i class=\"fas fa-home text-white fs-4\"></i>\n            </router-link>\n          </li>\n\n          <li class=\"nav-item d-none d-lg-block align-items-center mt-2\">\n            <span class=\"text-white mx-3\">|</span>\n          </li>\n\n          <li class=\"nav-item ms-2\" v-if=\"!isAuthenticated\">\n            <router-link class=\"nav-link btn-custom\" to=\"/login\">Accede</router-link>\n          </li>\n          <li class=\"nav-item ms-2\" v-if=\"!isAuthenticated\">\n            <router-link class=\"nav-link btn-custom\" to=\"/register\">Regístrate</router-link>\n          </li>\n\n          <!-- ✅ Menú de usuario -->\n          <UserMenu v-if=\"isAuthenticated\" :userName=\"userName\" :userProfilePicture=\"userProfilePicture\" @logout=\"logout\"/>\n\n          <!-- ✅ Campana de notificaciones -->\n          <Notifications v-if=\"isAuthenticated\" />\n        </ul>\n      </div>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport { useStore } from \"vuex\";\nimport { useRouter } from \"vue-router\";\nimport { computed } from \"vue\";\nimport UserMenu from \"./UserMenu.vue\";\nimport Notifications from \"./Notifications.vue\";\n\nexport default {\n  components: { UserMenu, Notifications },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n\n    const isAuthenticated = computed(() => store.getters[\"auth/isAuthenticated\"]);\n    const userName = computed(() => store.getters[\"auth/userData\"]?.name || \"Usuario\");\n    const userProfilePicture = computed(() => store.getters[\"auth/userData\"]?.profilePicture || \"default-profile.jpg\");\n\n    const isServiciosDisponiblesPage = computed(() => router.currentRoute.value.path === \"/client/services\");\n    const isDashboardPage = computed(() => router.currentRoute.value.path === \"/dashboard\");\n    const isHomePage = computed(() => router.currentRoute.value.path === \"/\");\n\n    const logout = () => {\n      store.dispatch(\"auth/logout\");\n      router.push(\"/login\");\n    };\n\n    return { isAuthenticated, userName, userProfilePicture, logout, isServiciosDisponiblesPage, isHomePage, isDashboardPage };\n  }\n};\n</script>\n\n<style scoped>\n.navbar { box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); }\n.navbar-brand { font-size: 1.5rem; letter-spacing: 1px; }\n.btn-custom { color:white; border-radius: 25px; padding: 8px 20px; transition: all 0.3s ease; }\n.btn-custom:hover { color: rgb(108, 233, 255); }\n</style>\n"],"mappings":"AAgDA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,QAAO,QAAS,KAAK;AAC9B,OAAOC,QAAO,MAAO,gBAAgB;AACrC,OAAOC,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACbC,UAAU,EAAE;IAAEF,QAAQ;IAAEC;EAAc,CAAC;EACvCE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIP,QAAQ,CAAC,CAAC;IACxB,MAAMQ,MAAK,GAAIP,SAAS,CAAC,CAAC;IAE1B,MAAMQ,eAAc,GAAIP,QAAQ,CAAC,MAAMK,KAAK,CAACG,OAAO,CAAC,sBAAsB,CAAC,CAAC;IAC7E,MAAMC,QAAO,GAAIT,QAAQ,CAAC,MAAMK,KAAK,CAACG,OAAO,CAAC,eAAe,CAAC,EAAEE,IAAG,IAAK,SAAS,CAAC;IAClF,MAAMC,kBAAiB,GAAIX,QAAQ,CAAC,MAAMK,KAAK,CAACG,OAAO,CAAC,eAAe,CAAC,EAAEI,cAAa,IAAK,qBAAqB,CAAC;IAElH,MAAMC,0BAAyB,GAAIb,QAAQ,CAA