{"ast":null,"code":"import axios from \"@/plugins/axios\";\nimport Notification from \"@/components/Notification.vue\";\nimport { useRouter } from \"vue-router\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"Register\",\n  components: {\n    Notification\n  },\n  setup() {\n    const router = useRouter();\n    const form = ref({\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      phone: \"\"\n    });\n    const successMessage = ref(\"\");\n    const errorMessage = ref(\"\");\n    const register = async () => {\n      try {\n        await axios.post(\"/auth/register\", form.value);\n        successMessage.value = \"Registro exitoso. ¡Por favor accede!\";\n        errorMessage.value = \"\";\n        form.value = {\n          name: \"\",\n          email: \"\",\n          password: \"\",\n          phone: \"\"\n        };\n        setTimeout(() => router.push(\"/login\"), 2000);\n      } catch (err) {\n        errorMessage.value = err.response?.data?.message || \"Error al registrarse.\";\n        successMessage.value = \"\";\n      }\n    };\n    const goHome = () => {\n      router.push(\"/\"); // Redirige a la página de inicio\n    };\n    return {\n      form,\n      successMessage,\n      errorMessage,\n      register,\n      goHome\n    };\n  }\n};","map":{"version":3,"names":["axios","Notification","useRouter","ref","name","components","setup","router","form","email","password","phone","successMessage","errorMessage","register","post","value","setTimeout","push","err","response","data","message","goHome"],"sources":["/Users/macbookpro/project/LatedServices/frontend/src/views/auth/Register.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h2 class=\"mb-4\">Crea tu cuenta</h2>\n    <Notification v-if=\"successMessage\" type=\"success\" :message=\"successMessage\" />\n    <Notification v-if=\"errorMessage\" type=\"danger\" :message=\"errorMessage\" />\n    <form @submit.prevent=\"register\">\n      <div class=\"form-group\">\n        <label for=\"name\">Nombre</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"name\"\n          v-model=\"form.name\"\n          placeholder=\"Ingrese su nombre\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"email\">Correo Electrónico</label>\n        <input\n          type=\"email\"\n          class=\"form-control\"\n          id=\"email\"\n          v-model=\"form.email\"\n          placeholder=\"Ingrese su correo\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"password\">Contraseña</label>\n        <input\n          type=\"password\"\n          class=\"form-control\"\n          id=\"password\"\n          v-model=\"form.password\"\n          placeholder=\"Ingrese su contraseña\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"phone\">Teléfono</label>\n        <input\n          type=\"tel\"\n          class=\"form-control\"\n          id=\"phone\"\n          v-model=\"form.phone\"\n          placeholder=\"Ingrese su número de teléfono\"\n        />\n      </div>\n      <button type=\"submit\" class=\"btn btn-primary\">Registrarse</button>\n      <p class=\"mt-3\">\n        ¿Ya tienes una cuenta? <router-link to=\"/login\">Accede</router-link>\n      </p>\n    </form>\n\n    <!-- Botón para volver a la página principal -->\n    <button @click=\"goHome\" class=\"btn btn-secondary mt-3\">Volver a Inicio</button>\n  </div>\n</template>\n\n<script>\nimport axios from \"@/plugins/axios\";\nimport Notification from \"@/components/Notification.vue\";\nimport { useRouter } from \"vue-router\";\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"Register\",\n  components: { Notification },\n  setup() {\n    const router = useRouter();\n    const form = ref({ name: \"\", email: \"\", password: \"\", phone: \"\" });\n    const successMessage = ref(\"\");\n    const errorMessage = ref(\"\");\n\n    const register = async () => {\n      try {\n        await axios.post(\"/auth/register\", form.value);\n        successMessage.value = \"Registro exitoso. ¡Por favor accede!\";\n        errorMessage.value = \"\";\n        form.value = { name: \"\", email: \"\", password: \"\", phone: \"\" };\n        setTimeout(() => router.push(\"/login\"), 2000);\n      } catch (err) {\n        errorMessage.value = err.response?.data?.message || \"Error al registrarse.\";\n        successMessage.value = \"\";\n      }\n    };\n\n    const goHome = () => {\n      router.push(\"/\"); // Redirige a la página de inicio\n    };\n\n    return { form, successMessage, errorMessage, register, goHome };\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 500px;\n}\n\n/* Espaciado y diseño del botón de regreso */\n.btn-secondary {\n  display: block;\n  width: max-content;\n  margin: 20px auto;\n  padding: 8px 20px;\n  font-size: 16px;\n  border-radius: 25px;\n  transition: all 0.3s ease;\n}\n\n.btn-secondary:hover {\n  background-color: #345896;\n  color:rgb(255, 255, 255);\n}\n</style>\n"],"mappings":"AA6DA,OAAOA,KAAI,MAAO,iBAAiB;AACnC,OAAOC,YAAW,MAAO,+BAA+B;AACxD,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEJ;EAAa,CAAC;EAC5BK,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,CAAC,CAAC;IAC1B,MAAMM,IAAG,GAAIL,GAAG,CAAC;MAAEC,IAAI,EAAE,EAAE;MAAEK,KAAK,EAAE,EAAE;MAAEC,QAAQ,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAG,CAAC,CAAC;IAClE,MAAMC,cAAa,GAAIT,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMU,YAAW,GAAIV,GAAG,CAAC,EAAE,CAAC;IAE5B,MAAMW,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAI;QACF,MAAMd,KAAK,CAACe,IAAI,CAAC,gBAAgB,EAAEP,IAAI,CAACQ,KAAK,CAAC;QAC9CJ,cAAc,CAACI,KAAI,GAAI,sCAAsC;QAC7DH,YAAY,CAACG,KAAI,GAAI,EAAE;QACvBR,IAAI,CAACQ,KAAI,GAAI;UAAEZ,IAAI,EAAE,EAAE;UAAEK,KAAK,EAAE,EAAE;UAAEC,QAAQ,EAAE,EAAE;UAAEC,KAAK,EAAE;QAAG,CAAC;QAC7DM,UAAU,CAAC,MAAMV,MAAM,CAACW,IAAI,CAAC,QAAQ,CAAC,EAAE,IAAI,CAAC;MAC/C,