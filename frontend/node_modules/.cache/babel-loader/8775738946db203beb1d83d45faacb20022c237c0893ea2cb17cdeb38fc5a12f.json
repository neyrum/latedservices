{"ast":null,"code":"import { useStore } from \"vuex\";\nimport { ref, onMounted } from \"vue\";\nimport { teamMembers } from \"@/data/teamData\"; // üîπ Importamos los miembros del equipo\n\nexport default {\n  name: \"Team\",\n  setup() {\n    const store = useStore();\n    const teamList = ref([]); // üîπ Lista vac√≠a al inicio\n\n    onMounted(async () => {\n      await store.dispatch(\"auth/fetchAllUsers\"); // üîπ Obtener usuarios desde la API\n      const allUsers = store.getters[\"auth/users\"] || []; // üîπ Garantiza que Vuex devuelve un array\n\n      // üîπ Asignar la imagen de perfil correcta a cada miembro del equipo\n      teamList.value = teamMembers.map(member => {\n        const user = allUsers.find(user => user.name === member.name);\n        return {\n          ...member,\n          image: user ? user.profilePicture : \"@assets/img/default-profile.jpg\"\n        };\n      });\n    });\n    return {\n      teamList\n    };\n  }\n};","map":{"version":3,"names":["useStore","ref","onMounted","teamMembers","name","setup","store","teamList","dispatch","allUsers","getters","value","map","member","user","find","image","profilePicture"],"sources":["/Users/macbookpro/project/LatedServices/frontend/src/components/Team.vue"],"sourcesContent":["<template>\n  <section class=\"page-section bg-light\" id=\"team\">\n    <div class=\"container\">\n      <div class=\"text-center\">\n        <h2 class=\"section-heading text-uppercase\">Nuestro Incre√≠ble Equipo</h2>\n        <h3 class=\"section-subheading text-muted\">Los talentos que hacen posible nuestro √©xito.</h3>\n      </div>\n      <div class=\"row\">\n        <div v-for=\"member in teamList\" :key=\"member.name\" class=\"col-lg-4\">\n          <div class=\"team-member\">\n            <img class=\"mx-auto rounded-circle\" :src=\"member.image\" alt=\"Foto de {{ member.name }}\" />\n            <h4>{{ member.name }}</h4>\n            <p class=\"text-muted\">{{ member.role }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { useStore } from \"vuex\";\nimport { ref, onMounted } from \"vue\";\nimport { teamMembers } from \"@/data/teamData\"; // üîπ Importamos los miembros del equipo\n\nexport default {\n  name: \"Team\",\n  setup() {\n    const store = useStore();\n    const teamList = ref([]); // üîπ Lista vac√≠a al inicio\n\n    onMounted(async () => {\n      await store.dispatch(\"auth/fetchAllUsers\"); // üîπ Obtener usuarios desde la API\n      const allUsers = store.getters[\"auth/users\"] || []; // üîπ Garantiza que Vuex devuelve un array\n\n      // üîπ Asignar la imagen de perfil correcta a cada miembro del equipo\n      teamList.value = teamMembers.map(member => {\n        const user = allUsers.find(user => user.name === member.name);\n        return { ...member, image: user ? user.profilePicture : \"@assets/img/default-profile.jpg\" };\n      });\n    });\n\n    return { teamList };\n  },\n};\n</script>\n"],"mappings":"AAqBA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,WAAU,QAAS,iBAAiB,EAAE;;AAE/C,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC,EAAE;;IAE1BC,SAAS,CAAC,YAAY;MACpB,MAAMI,KAAK,CAACE,QAAQ,CAAC,oBAAoB,CAAC,EAAE;MAC5C,MAAMC,QAAO,GAAIH,KAAK,CAACI,OAAO,CAAC,YAAY,KAAK,EAAE,EAAE;;MAEpD;MACAH,QAAQ,CAACI,KAAI,GAAIR,WAAW,CAACS,GAAG,CAACC,MAAK,IAAK;QACzC,MAAMC,IAAG,GAAIL,QAAQ,CAACM,IAAI,CAACD,IAAG,IAAKA,IAAI,CAACV,IAAG,KAAMS,MAAM,CAACT,IAAI,CAAC;QAC7D,OAAO;UAAE,GAAGS,MAAM;UAAEG,KAAK,EAAEF,IAAG,GAAIA,IAAI,CAACG,cAAa,GAAI;QAAkC,CAAC;MAC7F,CAAC,CAAC;IACJ,CAAC,CAAC;IAEF,OAAO;MAAEV;IAAS,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}