\"@/components/MaterialButton.vue\";\n\nexport default {\n  name: \"SignIn\",\n  components: {\n    Navbar,\n    MaterialInput,\n    MaterialSwitch,\n    MaterialButton,\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const form = ref({ email: \"\", password: \"\" });\n    const errorMessage = ref(\"\");\n\n    onMounted(() => {\n      if (store.getters[\"auth/isAuthenticated\"]) {\n        router.push(\"/client/services\");\n      }\n    });\n\n    const login = async () => {\n      try {\n        await store.dispatch(\"auth/login\", form.value);\n        router.push(\"/client/services\");\n      } catch (err) {\n        errorMessage.value = err.response?.data?.message || \"Error al iniciar sesión.\";\n      }\n    };\n\n    return { form, errorMessage, login };\n  },\n};\n</script>\n"],"mappings":"AA+FA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,MAAK,MAAO,kCAAkC;AACrD,OAAOC,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,cAAa,MAAO,iCAAiC;AAE5D,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVL,MAAM;IACNC,aAAa;IACbC,cAAc;IACdC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIX,QAAQ,CAAC,CAAC;IACxB,MAAMY,MAAK,GAAIX,SAAS,CAAC,CAAC;IAC1B,MAAMY,IAAG,GAAIX,GAAG,CAAC;MAAEY,KAAK,EAAE,EAAE;MAAEC,QAAQ,EAAE;IAAG,CAAC,CAAC;IAC7C,MAAMC,YAAW,GAAId,GAAG,CAAC,EAAE,CAAC;IAE5BC,SAAS,CAAC,MAAM;MACd,IAAIQ,KAAK,CAACM,OAAO,CAAC,sBAAsB,CAAC,EAAE;QACzCL,MAAM,CAACM,IAAI,CAAC,kBAAkB,CAAC;MACjC;IACF,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAI,MAAAA,CAAA,KAAY;MACxB,IAAI;QACF,MAAMR,KAAK,CAACS,QAAQ,CAAC,YAAY,EAAEP,IAAI,CAACQ,KAAK,CAAC;QAC9CT,MAAM,CAACM,IAAI,CAAC,kBAAkB,CAAC;MACjC,EAAE,OAAOI,GAAG,EAAE;QACZN,YAAY,CAACK,KAAI,GAAIC,GAAG,CAACC,QAAQ,EAAEC,IAAI,EAAEC,OAAM,IAAK,0BAA0B;MAChF;IACF,CAAC;IAED,OAAO;MAAEZ,IAAI;MAAEG,YAAY;MAAEG;IAAM,CAAC;EACtC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}